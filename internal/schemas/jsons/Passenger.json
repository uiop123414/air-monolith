{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "surname": {
      "type": "string"
    },
    "patronymic": {
      "type": "string"
    },
    "doc_type": {
      "type": "string"
    },
    "doc_number": {
      "type": "string"
    },
    "birthdate": {
      "$ref": "./Birthdate.json"
    },
    "gender": {
      "type": "string"
    },
    "passenger_type": {
      "type": "string"
    },
    "ticket_number": {
      "type": "string"
    },
    "ticket_type": {
      "type": "integer"
    }
